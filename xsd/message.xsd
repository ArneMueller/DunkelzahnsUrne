<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:m="http://id.piratenpartei.de/message"        
        xmlns:sig="http://www.w3.org/2000/09/xmldsig#"
        targetNamespace="http://id.piratenpartei.de/message"
        elementFormDefault="qualified">
  <import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="http://www.w3.org/2000/09/xmldsig#"/>
  <element name="Message">
    <complexType>
      <sequence>
        <element ref="m:Body"/>
        <element ref="sig:Signature"/>
      </sequence>
    </complexType>
  </element>
  <element name="Body">
    <complexType>
		<sequence>
			<any minOccurs="1" maxOccurs="unbounded" processContents="skip"/>
		</sequence>
		<attribute name="author" type="string"  use="required"/>
		<attribute name="id" type="ID" use="required"/>
    </complexType>
  </element>
</schema>
